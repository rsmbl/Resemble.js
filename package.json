        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-prettier": "^2.6.0",
        "husky": "^0.14.3",
        "jest": "^20.0.4",
        "jest-cli": "^20.0.4",
        "lint-staged": "^7.0.0",
        "prettier": "^1.11.1"
